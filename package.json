{
  "name": "@pilatos/bitbucket-cli",
  "version": "1.3.1",
  "description": "A command-line interface for Bitbucket Cloud",
  "author": "",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/0pilatos0/bitbucket-cli.git"
  },
  "bugs": {
    "url": "https://github.com/0pilatos0/bitbucket-cli/issues"
  },
  "homepage": "https://github.com/0pilatos0/bitbucket-cli#readme",
  "bin": {
    "bb": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "bun": ">=1.0"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "test": "bun test",
    "lint": "tsc --noEmit",
    "generate:api": "openapi-generator-cli generate",
    "docs:dev": "cd docs && bun run dev",
    "docs:build": "cd docs && bun run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "bun run build && changeset publish",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "tabtab": "^3.0.2"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@openapitools/openapi-generator-cli": "^2.28.0",
    "@types/bun": "latest",
    "@types/node": "^25.1.0",
    "typescript": "^5.9.3"
  }
}
