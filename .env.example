# Bitbucket CLI Configuration
# Copy this file to .env and fill in your values

# Bitbucket username
BB_USERNAME=

# Bitbucket API token (create at: https://bitbucket.org/account/settings/api-tokens/)
# Note: App passwords are deprecated as of September 9, 2025. Use API tokens instead.
BB_API_TOKEN=

# Default workspace (optional)
BB_WORKSPACE=
